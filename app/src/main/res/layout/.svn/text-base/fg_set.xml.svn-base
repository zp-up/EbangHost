<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:background="#EDEDED"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">
    <!--头部-->
    <RelativeLayout
        android:background="@color/app_green"
        android:layout_width="match_parent"
        android:layout_height="40dp">
        <TextView
            android:text="设置"
            android:layout_centerInParent="true"
            android:textSize="18sp"
            android:textColor="@color/app_white"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />

    </RelativeLayout>
    <!--商铺信息页面-->
    <LinearLayout
        android:background="@color/app_green"
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="100dp">
        <!--商户头像-->
        <LinearLayout
            android:layout_gravity="center"
            android:layout_marginLeft="20dp"
            android:layout_width="80dp"
            android:layout_height="80dp">
            <ImageView
                android:id="@+id/fg_set_iv_logo"
                android:layout_width="match_parent"
                android:layout_height="match_parent" />
        </LinearLayout>
        <!--商户信息-->
        <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <TextView
                android:id="@+id/fg_set_tv_name"
            android:text="name"
            android:textColor="@color/app_white"
            android:textSize="18sp"
            android:layout_marginLeft="40dp"
            android:layout_marginTop="10dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />
           <LinearLayout
               android:layout_marginTop="10dp"
               android:orientation="horizontal"
               android:layout_width="match_parent"
               android:layout_height="20dp">
               <ImageView
                   android:src="@mipmap/icon_set_position"
                   android:layout_marginLeft="10dp"
                   android:layout_width="20dp"
                   android:layout_height="20dp" />
               <TextView
                   android:id="@+id/fg_set_tv_address"
                   android:text="address"
                   android:textColor="@color/app_white"
                   android:textSize="15sp"
                  android:layout_marginLeft="10dp"
                   android:layout_width="wrap_content"
                   android:layout_height="wrap_content" />
           </LinearLayout>
            <LinearLayout
                android:layout_marginTop="5dp"
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="20dp">
                <ImageView
                    android:src="@mipmap/icon_set_phone"
                    android:layout_marginLeft="10dp"
                    android:layout_width="20dp"
                    android:layout_height="20dp" />
                <TextView
                    android:id="@+id/fg_set_tv_phone"
                    android:text="phone"
                    android:textColor="@color/app_white"
                    android:textSize="15sp"
                    android:layout_marginLeft="10dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
    <Space
        android:layout_width="match_parent"
        android:layout_height="10dp" />
    <!--设置1-->
    <LinearLayout
        android:orientation="vertical"
        android:background="@color/app_white"
        android:layout_width="match_parent"
        android:layout_height="80dp">
        <RelativeLayout
            android:id="@+id/activity_set_music"
            android:layout_weight="1"
            android:layout_width="match_parent"
            android:layout_height="0dp">
            <ImageView
                android:id="@+id/img_1"
                android:layout_centerVertical="true"
                android:src="@mipmap/icon_set_img1"
                android:layout_marginLeft="10dp"
                android:layout_width="25dp"
                android:layout_height="25dp" />

            <ImageView
                android:id="@+id/img_1_back"
                android:src="@mipmap/icon_set_rightback"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="10dp"
                android:layout_width="20dp"
                android:layout_height="20dp" />

            <TextView
                android:text="提示音设置"
                android:textColor="#000000"
                android:textSize="18sp"
                android:layout_marginLeft="10dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignTop="@+id/img_1_back"
                android:layout_toEndOf="@+id/img_1"
                android:id="@+id/textView" />
        </RelativeLayout>
        <View
        android:layout_width="match_parent"
        android:layout_marginLeft="10dp"
        android:background="@color/app_gray"
        android:layout_height="1dp"></View>
        <RelativeLayout
            android:layout_weight="1"
            android:layout_width="match_parent"
            android:layout_height="0dp">
            <ImageView
                android:id="@+id/img_2_zd"
                android:layout_centerVertical="true"
                android:src="@mipmap/icon_set_img2"
                android:layout_marginLeft="10dp"
                android:layout_width="25dp"
                android:layout_height="25dp" />
            <TextView
                android:text="手机震动提醒"
                android:textColor="#000000"
                android:textSize="18sp"
                android:layout_marginLeft="10dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_toEndOf="@+id/img_2_zd" />
            <com.zcw.togglebutton.ToggleButton
                android:layout_marginRight="10dp"
                xmlns:toggle="http://schemas.android.com/apk/res-auto"
                android:layout_centerVertical="true"
                android:layout_alignParentRight="true"
                android:layout_width="wrap_content"
                android:layout_height="25dp" />

        </RelativeLayout>

    </LinearLayout>

    <Space
    android:layout_width="match_parent"
    android:layout_height="10dp" />
    <!--设置2-->
    <LinearLayout
        android:orientation="vertical"
        android:background="@color/app_white"
        android:layout_width="match_parent"
        android:layout_height="120dp">
        <RelativeLayout
            android:layout_weight="1"
            android:layout_width="match_parent"
            android:layout_height="0dp">
            <ImageView
                android:id="@+id/img_2"
                android:layout_centerVertical="true"
                android:src="@mipmap/icon_set_img3"
                android:layout_marginLeft="10dp"
                android:layout_width="25dp"
                android:layout_height="25dp" />

            <ImageView
                android:id="@+id/img_2_back"
                android:src="@mipmap/icon_set_rightback"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="10dp"
                android:layout_width="20dp"
                android:layout_height="20dp" />
            <TextView
                android:textColor="@color/text_black"
                android:layout_toLeftOf="@id/img_2_back"
                android:layout_marginRight="10dp"
                android:text="周经理"
                android:textSize="18sp"
                android:layout_centerVertical="true"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />

            <TextView
                android:text="联系市场经理"
                android:textColor="#000000"
                android:textSize="18sp"
                android:layout_marginLeft="10dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignTop="@+id/img_2_back"
                android:layout_toEndOf="@+id/img_2"
                />
        </RelativeLayout>
        <View
            android:layout_width="match_parent"
            android:layout_marginLeft="10dp"
            android:background="@color/app_gray"
            android:layout_height="1dp"></View>
        <RelativeLayout
            android:layout_weight="1"
            android:layout_width="match_parent"
            android:layout_height="0dp">
            <ImageView
                android:id="@+id/img_3"
                android:layout_centerVertical="true"
                android:src="@mipmap/icon_set_img4"
                android:layout_marginLeft="10dp"
                android:layout_width="25dp"
                android:layout_height="25dp" />

            <ImageView
                android:id="@+id/img_3_back"
                android:src="@mipmap/icon_set_rightback"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="10dp"
                android:layout_width="20dp"
                android:layout_height="20dp" />
            <TextView
                android:textColor="@color/text_black"
                android:layout_toLeftOf="@id/img_3_back"
                android:layout_marginRight="10dp"
                android:text="028-58301347"
                android:textSize="18sp"
                android:layout_centerVertical="true"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />

            <TextView
                android:text="联系客服"
                android:textColor="#000000"
                android:textSize="18sp"
                android:layout_marginLeft="10dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignTop="@+id/img_3_back"
                android:layout_toEndOf="@+id/img_3"
                 />
        </RelativeLayout>
        <View
            android:layout_width="match_parent"
            android:layout_marginLeft="10dp"
            android:background="@color/app_gray"
            android:layout_height="1dp"></View>
        <RelativeLayout
            android:id="@+id/activity_set_advice"
            android:layout_weight="1"
            android:layout_width="match_parent"
            android:layout_height="0dp">
            <ImageView
                android:id="@+id/img_4"
                android:layout_centerVertical="true"
                android:src="@mipmap/icon_set_img5"
                android:layout_marginLeft="10dp"
                android:layout_width="25dp"
                android:layout_height="25dp" />

            <ImageView
                android:id="@+id/img_4_back"
                android:src="@mipmap/icon_set_rightback"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="10dp"
                android:layout_width="20dp"
                android:layout_height="20dp" />
            <TextView
                android:textColor="@color/text_black"
                android:layout_toLeftOf="@id/img_4_back"
                android:layout_marginRight="10dp"
                android:text=""
                android:textSize="18sp"
                android:layout_centerVertical="true"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />

            <TextView
                android:text="使用反馈"
                android:textColor="#000000"
                android:textSize="18sp"
                android:layout_marginLeft="10dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignTop="@+id/img_4_back"
                android:layout_toEndOf="@+id/img_4"
                />
        </RelativeLayout>
    </LinearLayout>
    <Space
        android:layout_width="match_parent"
        android:layout_height="10dp" />
    <!--设置3-->
    <LinearLayout
        android:background="@color/app_white"
        android:layout_width="match_parent"
        android:layout_height="40dp">
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <ImageView
                android:id="@+id/img_6"
                android:layout_centerVertical="true"
                android:src="@mipmap/icon_set_img6"
                android:layout_marginLeft="10dp"
                android:layout_width="25dp"
                android:layout_height="25dp" />

            <ImageView
                android:id="@+id/img_6_back"
                android:src="@mipmap/icon_set_rightback"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="10dp"
                android:layout_width="20dp"
                android:layout_height="20dp" />
            <TextView
                android:textColor="@color/text_black"
                android:layout_toLeftOf="@id/img_6_back"
                android:layout_marginRight="10dp"
                android:text="V 1.0"
                android:textSize="18sp"
                android:layout_centerVertical="true"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />

            <TextView
                android:text="软件版本"
                android:textColor="#000000"
                android:textSize="18sp"
                android:layout_marginLeft="10dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignTop="@+id/img_6_back"
                android:layout_toEndOf="@+id/img_6"
                 />
        </RelativeLayout>
    </LinearLayout>
    <Space
        android:layout_width="match_parent"
        android:layout_height="10dp" />
    <Button
        android:id="@+id/fg_set_btn_logout"
        android:background="@color/app_white"
        android:textColor="#7C7C7C"
        android:text="退出登录"
        android:textSize="20sp"
        android:layout_width="match_parent"
        android:layout_height="50dp" />
</LinearLayout>