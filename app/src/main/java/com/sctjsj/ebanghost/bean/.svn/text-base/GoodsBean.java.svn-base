import java.util.List;

/**
 * 商品实体
 */
public class GoodsBean {
    private int id;//id
    private String name;//名字
    private double price;//价格
    private int count;//总数
    private String description;//商品描述
    private String logoUrl;//logo
    private int status;//商品状态

    private List<ClassifyBean> classifyBeanList;

    public int getStatus() {
        return status;
    }

    public void setStatus(int status) {
        this.status = status;
    }

    public String getLogoUrl() {
        return logoUrl;
    }

    public void setLogoUrl(String logoUrl) {
        this.logoUrl = logoUrl;
    }

    public String getDescription() {
        return description;
    }

    public void setDescription(String description) {
        this.description = description;
    }


    public List<ClassifyBean> getClassifyBeanList() {
        return classifyBeanList;
    }

    public void setClassifyBeanList(List<ClassifyBean> classifyBeanList) {
        this.classifyBeanList = classifyBeanList;
    }

    public GoodsBean() {
    }

    public int getId() {
        return id;
    }

    public void setId(int id) {
        this.id = id;
    }

    public String getName() {
        return name;
    }

    public void setName(String name) {
        this.name = name;
    }

    public int getCount() {
        return count;
    }

    public void setCount(int count) {
        this.count = count;
    }

    public double getPrice() {
        return price;
    }

    public void setPrice(double price) {
        this.price = price;
    }
}
